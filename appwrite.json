{
    "projectId": "almost-vault",
    "endpoint": "https://fra.cloud.appwrite.io/v1",
    "functions": [
        {
            "$id": "api",
            "name": "api",
            "runtime": "node-22",
            "specification": "s-4vcpu-4gb",
            "execute": [
                "any"
            ],
            "events": [],
            "scopes": [
                "documents.read",
                "documents.write"
            ],
            "schedule": "",
            "timeout": 30,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "function"
        }
    ],
    "sites": [
        {
            "$id": "webapp",
            "name": "webapp",
            "framework": "vite",
            "adapter": "static",
            "buildRuntime": "node-22",
            "installCommand": "npm install",
            "buildCommand": "npm run build",
            "outputDirectory": "./dist",
            "fallbackFile": "index.html",
            "specification": "s-4vcpu-4gb",
            "enabled": true,
            "timeout": 30,
            "logging": true,
            "path": "site"
        }
    ],
    "projectName": "almost-vault",
    "settings": {
        "services": {
            "account": false,
            "avatars": false,
            "databases": true,
            "locale": false,
            "health": true,
            "storage": false,
            "teams": false,
            "users": true,
            "sites": true,
            "functions": true,
            "graphql": true,
            "messaging": false
        },
        "auth": {
            "methods": {
                "jwt": false,
                "phone": false,
                "invites": false,
                "anonymous": false,
                "email-otp": false,
                "magic-url": false,
                "email-password": false
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false,
                "sessionAlerts": false,
                "mockNumbers": []
            }
        }
    },
    "databases": [
        {
            "$id": "main",
            "name": "main",
            "enabled": true
        }
    ],
    "tables": [
        {
            "$id": "secrets",
            "$permissions": [],
            "databaseId": "main",
            "name": "secrets",
            "enabled": true,
            "columns": [
                {
                    "key": "reads",
                    "type": "integer",
                    "required": true,
                    "array": false,
                    "min": 1,
                    "max": 1000,
                    "default": null
                },
                {
                    "key": "ttl",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "elements": [
                        "hour",
                        "day",
                        "week",
                        "month",
                        "year",
                        "mock"
                    ],
                    "format": "enum",
                    "default": null
                },
                {
                    "key": "secret",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 2048,
                    "default": null,
                    "encrypt": true
                }
            ],
            "indexes": []
        }
    ]
}